<Application x:Class="EtAlii.Ubigia.Windows.Diagnostics.SpaceBrowser.App"
    xmlns:f="clr-namespace:Fluent;assembly=Fluent"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Startup="OnApplicationStartup"
    Exit="OnApplicationExit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Fluent;Component/Themes/generic.xaml" />
                <ResourceDictionary Source="pack://application:,,,/EtAlii.Ubigia.Windows;component\Icons.xaml" />
                <ResourceDictionary Source="/Resources.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <Color x:Key="{x:Static f:MetroColors.ThemeColorKey}">#359900</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>
