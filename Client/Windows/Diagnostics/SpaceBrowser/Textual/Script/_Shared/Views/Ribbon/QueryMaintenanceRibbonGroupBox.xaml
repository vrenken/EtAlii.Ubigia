<fluent:RibbonGroupBox 
    x:Class="EtAlii.Ubigia.Windows.Diagnostics.SpaceBrowser.QueryMaintenanceRibbonGroupBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:fluent="clr-namespace:Fluent;assembly=Fluent"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    mc:Ignorable="d" 
    d:DesignHeight="300" d:DesignWidth="300"
    fluent:KeyTip.Keys="A"
    Header="Actions">

    <!--DataContext="{Binding Buttons}"-->
    <fluent:Button 
        Header="Clear current query"
        Command="{Binding ClearCommand}"
        CommandParameter="{Binding Code}"
        LargeIcon="pack://siteoforigin:,,,/Images/Garbage-Closed.png" />
</fluent:RibbonGroupBox>
