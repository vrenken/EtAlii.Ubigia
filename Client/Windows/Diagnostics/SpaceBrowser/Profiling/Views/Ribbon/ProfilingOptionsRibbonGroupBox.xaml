<fluent:RibbonGroupBox 
    x:Class="EtAlii.Ubigia.Windows.Diagnostics.SpaceBrowser.ProfilingOptionsRibbonGroupBox"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:fluent="clr-namespace:Fluent;assembly=Fluent"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    mc:Ignorable="d" 
    d:DesignHeight="300" d:DesignWidth="300"
    fluent:KeyTip.Keys="O"
    Header="Options">

    <!--DataContext="{Binding Buttons}"-->
    <fluent:Button 
        Header="Scroll to entries"
        Command="{Binding ClearCommand}"
        CommandParameter="{Binding Code}"
        LargeIcon="pack://siteoforigin:,,,/Images/Navigation-Down.png" />
    <fluent:ToggleButton 
        IsChecked="{Binding AutoExpandNodes}"
        Header="Expand new entries"
        LargeIcon="pack://siteoforigin:,,,/Images/Navigation-Right.png" />
</fluent:RibbonGroupBox>
