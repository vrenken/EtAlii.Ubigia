<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>$(UbigiaNetCoreAppVersion)</TargetFramework>
    <Description>Package that contains the implementation needed to store data on the NTFS filesystem.</Description>
    <Version>$(PackageVersion)</Version>
    <PackageProjectUrl>http://petervrenken.nl/components/EtAlii.Ubigia.Persistence.Ntfs</PackageProjectUrl>
    <PackageReleaseNotes>Migrated to .NET Standard 2.0</PackageReleaseNotes>
    <PackageTags>Storage Ubigia Ntfs</PackageTags>
    <GeneratePackageOnBuild>$(UbigiaBuildPackages)</GeneratePackageOnBuild>
    <ProjectGuid>2FB9D0D5-DE7B-44EB-AC89-81DE306D9554</ProjectGuid>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Experimental.IO" Version="1.0.0">
      <NoWarn>NU1701</NoWarn>
    </PackageReference>
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\EtAlii.Ubigia.Persistence\EtAlii.Ubigia.Persistence.csproj" />
  </ItemGroup>
</Project>
